{
  "port": 3000,
  "logger": {
    "console": {
      "level": "debug",
      "colorize": true,
      "timestamp": true
    },
    "file": {
      "level": "debug",
      "timestamp": true,
      "filename": "logs/server.log",
      "json": false,
      "maxsize": "10MB",
      "tailable": true
    },
    "rotate": {
      "level": "debug",
