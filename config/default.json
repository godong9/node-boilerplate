      "timestamp": true,
      "filename": "logs/server.log",
      "datePattern": "-yyyy-MM-dd",
      "maxFiles": 10
    }
  },
  "database": {
    "username": "test",
    "password": "test",
    "database": "content",
    "host": "127.0.0.1",
    "dialect": "mysql"
  }
}